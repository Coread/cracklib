msgid "index magic fseek failed\n"
msgstr "selhání při hledání indexu magického čísla\n"

#: ../cracklib/packlib.c:138
msgid "index magic fwrite failed\n"
msgstr "chyba při zápisu indexu magického čísla\n"

#: ../cracklib/packlib.c:254
msgid "(index fseek failed)"
msgstr "(selhalo hledání indexu)"

#: ../cracklib/packlib.c:260
