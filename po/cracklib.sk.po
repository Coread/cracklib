msgid "it's derived from your password entry"
msgstr "je odvodené zo záznamu o účte"

#: ../cracklib/fascist.c:599
msgid "it is derivable from your password entry"
msgstr "je odvoditeľné zo záznamu o účte"

#: ../cracklib/fascist.c:608
msgid "it's derivable from your password entry"
msgstr "je odvoditeľné zo záznamu o účte"

#: ../cracklib/fascist.c:638
msgid "it's WAY too short"
msgstr "je PRÍLIŠ krátke"

#: ../cracklib/fascist.c:643
msgid "it is too short"
msgstr "je príliš krátke"

#: ../cracklib/fascist.c:660
msgid "it does not contain enough DIFFERENT characters"
